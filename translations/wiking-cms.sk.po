msgid "Publish the page in its current state"
msgstr "Zverejeniť súčasnú podobu stránky"

#: ../lib/wiking/cms/cms.py:1097
msgid "Unpublish"
msgstr "Nezverejeniť"

#: ../lib/wiking/cms/cms.py:1097
msgid "Make the page invisible from outside"
msgstr "Učiniť stránku neviditeľnou zvonku"

