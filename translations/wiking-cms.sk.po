#: ../lib/wiking/cms/cms.py:949
msgid "Activate the default color theme"
msgstr "Aktivovať východzí farebný motív"

#: ../lib/wiking/cms/cms.py:973
msgid "Default"
msgstr "Východzie"

#: ../lib/wiking/cms/cms.py:977
