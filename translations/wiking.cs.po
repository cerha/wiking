#: ../lib/wiking/modules.py:756 ../lib/wiking/modules.py:757
msgid "Page"
msgstr "Stránka"

#: ../lib/wiking/modules.py:762 ../lib/wiking/modules.py:991
#: ../lib/wiking/modules.py:1004
msgid "File"
msgstr "Soubor"

#: ../lib/wiking/modules.py:767
msgid ""
"Upload a file from your local system.  The file name will be used to refer "
"to the attachment within the page content."
msgstr ""
"Nahrajte soubor z Vašeho disku.  Název tohoto souboru bude používán pro\n"
"odkazování na tuto přílohu v rámci obsahu stránky."

#: ../lib/wiking/modules.py:770
msgid "Filename"
msgstr "Název souboru"

#: ../lib/wiking/modules.py:772
msgid "Mime-type"
msgstr "Typ MIME"

#: ../lib/wiking/modules.py:775
msgid ""
"The name of the attachment (e.g. the full name of the document). If empty, "
"the file name will be used instead."
msgstr ""
"Název přílohy (např. plný název dokumentu).  Pokud není zadáno, bude "
"zobrazen\n"
"přímo název souboru."

#: ../lib/wiking/modules.py:778 ../lib/wiking/modules.py:1009
#: ../lib/wiking/modules.py:1094
msgid "Description"
msgstr "Popis"

#: ../lib/wiking/modules.py:779
