#: ../lib/wiking/db.py:40
msgid "This value already exists.  Enter a unique value."
msgstr "Taková hodnota již existuje.  Zadejte unikátní hodnotu."

#: ../lib/wiking/db.py:42
msgid "Empty value.  This field is mandatory."
msgstr "Prázdná hodnota.  Tato položka je povinná."

#: ../lib/wiking/db.py:45
msgid "New record"
msgstr "Nový záznam"

#: ../lib/wiking/db.py:46
msgid "Create a new record"
msgstr "Vytvořit nový záznam"

#: ../lib/wiking/db.py:47
msgid "Edit"
msgstr "Upravit"

#: ../lib/wiking/db.py:48
msgid "Modify the record"
msgstr "Změnit záznam"

#: ../lib/wiking/db.py:49
msgid "Remove"
msgstr "Smazat"

#: ../lib/wiking/db.py:50
msgid "Remove the record permanently"
msgstr "Nevratně odstranit záznam"

#: ../lib/wiking/db.py:51
msgid "Please, confirm removing the record permanently."
msgstr "Potvrďte, prosím, nevratné smazání záznamu."

#: ../lib/wiking/db.py:52
msgid "Copy"
msgstr "Vytvořit kopii"

#: ../lib/wiking/db.py:53
msgid "Create new record initialized by values of this record"
msgstr "Vytvořit nový záznam předvyplněný hodnotami tohoto záznamu"

#: ../lib/wiking/db.py:54
msgid "Back to list"
msgstr "Zpět na seznam"

#: ../lib/wiking/db.py:55
msgid "Back to the list of all records"
msgstr "Zpět na výpis všech záznamů"

#: ../lib/wiking/db.py:56
msgid "New record was successfully inserted."
msgstr "Nový záznam byl úspěšně vložen."

#: ../lib/wiking/db.py:57
msgid "The record was successfully updated."
msgstr "Záznam byl úspěšně aktualizován."

#: ../lib/wiking/db.py:58
msgid "The record was deleted."
msgstr "Záznam byl odstraněn."

#: ../lib/wiking/db.py:242
msgid "Invalid binding column value."
msgstr "Nepřípustná hodnota vazebního sloupce."

#: ../lib/wiking/db.py:293
msgid "Integrity check failed."
msgstr "Kontrola integrity dat selhala."

#: ../lib/wiking/db.py:660
msgid "This function is temporarily unavailable."
msgstr "Tato funkce je dočasně nedostupná."

#: ../lib/wiking/db.py:930
msgid "Back"
msgstr "Zpět"

#: ../lib/wiking/db.py:1022
msgid "An RSS channel is available for this section:"
msgstr "K dispozici je také RSS kanál pro tuto sekci:"

#: ../lib/wiking/db.py:1026
msgid "more about RSS"
msgstr "více o RSS"

#: ../lib/wiking/db.py:1102
msgid "No records."
msgstr "Žádné záznamy"

#: ../lib/wiking/db.py:1109
msgid "The item was published."
msgstr "Položka byla zveřejněna."

#: ../lib/wiking/db.py:1110
msgid "The item was unpublished."
msgstr "Zveřejnění položky bylo zrušeno."

#: ../lib/wiking/db.py:1119
msgid "Publish"
msgstr "Zveřejnit"

#: ../lib/wiking/db.py:1122
msgid "Make the item visible to website visitors"
msgstr "Zpřístupnit položku návštěvníkům stránek"

#: ../lib/wiking/db.py:1123
msgid "Unpublish"
msgstr "Zneveřejnit"

#: ../lib/wiking/db.py:1126
msgid "Make the item invisible to website visitors"
msgstr "Učinit položku nedostupnou pro návštěvníky stránek"

#: ../lib/wiking/handler.py:95 ../lib/wiking/util.py:193
msgid "The server was unable to complete your request."
msgstr "Server nebyl schopen obsloužit Váš požadavek."

#: ../lib/wiking/handler.py:96 ../lib/wiking/util.py:194
#, python-format
msgid ""
"Please inform the server administrator, %(admin)s if the problem persists."
msgstr "Informujte prosím správce serveru, %(admin)s pokud problém přetrvává."

#: ../lib/wiking/handler.py:98 ../lib/wiking/util.py:197
msgid "The error message was:"
msgstr "Chybová zpráva byla:"

#: ../lib/wiking/util.py:56
msgid "Authentication required"
msgstr "Akce vyžaduje přihlášení"

#: ../lib/wiking/util.py:64 ../lib/wiking/util.py:401
msgid "Your password expired"
msgstr "Platnost hesla vypršela"

#: ../lib/wiking/util.py:67
msgid ""
"Your password expired.  Access to the application is now blocked for "
"security reasons until you change your password."
msgstr ""
"Platnost Vašeho hesla vypršela. Přístup k aplikaci bude z bezpečnostních "
"důvodů až do změny hesla zablokován."

#. Translators: Login panel link.
#: ../lib/wiking/util.py:71 ../lib/wiking/util.py:409
msgid "Change your password"
msgstr "Změnit heslo"

#. Translators: Login dialog page title (use a noun).
#. Translators: Login info label (noun) followed by login name and login controls.
#: ../lib/wiking/util.py:79 ../lib/wiking/util.py:422
#: ../lib/wiking/util.py:747
msgid "Login"
msgstr "Přihlášení"

#: ../lib/wiking/util.py:85
msgid "Access Denied"
msgstr "Přístup odepřen"

#: ../lib/wiking/util.py:88
msgid "You don't have sufficient privilegs for this action."
msgstr "Nemáte dostatečná práva pro provedení této akce."

#: ../lib/wiking/util.py:89
#, python-format
msgid ""
"If you are sure that you are logged in under the right account and you "
"believe that this is a problem of access rights assignment, please contact "
"the administrator at %s."
msgstr ""
"Pokud jste si jisti, že jste přihlášeni pod správným účtem a domníváte se, "
"že se jedná o chybu nastavení, kontaktujte, prosím, správce na adrese %s."

#. Translators: '%(code)d' is replaced by error number and '%(name)s' by error title.
#: ../lib/wiking/util.py:115
#, python-format
msgid "Error %(code)d: %(name)s"
msgstr "Chyba %(code)d: %(name)s"

#: ../lib/wiking/util.py:138
msgid "Invalid request arguments."
msgstr "Neplatné argumenty dotazu."

#. Translators: The word 'item' is intentionaly very generic, since it may mean a page,
#. image, streaming video, RSS channel or anything else.
#: ../lib/wiking/util.py:148
#, python-format
msgid "The item '%s' does not exist on this server or cannot be served."
msgstr ""
"Požadovaná položka '%s' neexistuje na tomto serveru nebo nemůže být "
"obsloužena."

#: ../lib/wiking/util.py:150
#, python-format
msgid ""
"If you are sure the web address is correct, but are encountering this error, "
"please contact the administrator at %s."
msgstr ""
"Pokud jste si jisti, že zadaná adresa je správná a přesto dochází k této\n"
"chybě, kontaktujte prosím správce na adrese %s."

#: ../lib/wiking/util.py:162
#, python-format
msgid "The item '%s' is not available."
msgstr "Položka '%s' není dostupná."

#: ../lib/wiking/util.py:163
msgid "The item exists on the server, but can not be accessed."
msgstr "Požadovaná položka na serveru existuje, ale nemůže být zobrazena."

#: ../lib/wiking/util.py:170 ../lib/wiking/export.py:66
#: ../lib/wiking/export.py:173
msgid "Language selection"
msgstr "Výběr jazyka"

#: ../lib/wiking/util.py:173
#, python-format
msgid ""
"The resource '%s' is not available in either of the requested languages."
msgstr "Požadovaná položka '%s' není dostupná v žádném z požadovaných jazyků."

