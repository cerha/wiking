#: ../lib/wiking/db.py:41
msgid "This value already exists.  Enter a unique value."
msgstr "Este valor ya existe. Introduzca un valor único."

#. Translators: This is about an empty (not filled in) value in a web form. Field means a form field.
#: ../lib/wiking/db.py:44
msgid "Empty value.  This field is mandatory."
msgstr "Valor vacío. Este campo es obligatorio."

#. Translators: Button label. Record as in ,,database record''.
#: ../lib/wiking/db.py:48
msgid "New record"
msgstr "Nuevo registro"

#. Translators: Button label. Record as in ,,database record''.
#: ../lib/wiking/db.py:50
msgid "Create a new record"
msgstr "Crear un nuevo registro"

#. Translators: Edit button label. Computer terminology and form common for a webpage button
#: ../lib/wiking/db.py:52
msgid "Edit"
msgstr "Editar"

#. Translators: Button label. Record as in ,,database record''.
#: ../lib/wiking/db.py:54
msgid "Modify the record"
msgstr "Modificar el registro"

#. Translators: Button label.
#: ../lib/wiking/db.py:56
msgid "Remove"
msgstr "Eliminar"

#. Translators: Button label. Record as in ,,database record''.
#: ../lib/wiking/db.py:58
msgid "Remove the record permanently"
msgstr "Eliminar el registro permanentemente"

#. Translators: Record as in ,,database record''.
#: ../lib/wiking/db.py:60
msgid "Please, confirm removing the record permanently."
msgstr "Por favor, confirme eliminar el registro permantemente."

#. Translators: Copy button label. Computer terminology and form common for a webpage button.
#: ../lib/wiking/db.py:62
msgid "Copy"
msgstr "Copiar"

#. Translators: Record as in ,,database record''.
#: ../lib/wiking/db.py:64
msgid "Create new record initialized by values of this record"
msgstr "Crear un nuevo registro inicializado con valores de este registro"

#. Translators: Button label.
#: ../lib/wiking/db.py:66
msgid "Back to list"
msgstr "Volver a la lista"

#. Translators: Record as in ,,database record''.
#: ../lib/wiking/db.py:68
msgid "Back to the list of all records"
msgstr "Volver a la lista de todos los registros"

#. Translators: Record as in ,,database record''.
#: ../lib/wiking/db.py:70
msgid "New record was successfully inserted."
msgstr "Nuevo registro ha sido insertado con éxito."

#. Translators: Record as in ,,database record''.
#: ../lib/wiking/db.py:72
msgid "The record was successfully updated."
msgstr "El registro ha sido actualizado con éxito."

#. Translators: Record as in ,,database record''.
#: ../lib/wiking/db.py:74
msgid "The record was deleted."
msgstr "El registro ha sido borrado."

#: ../lib/wiking/db.py:258
msgid "Invalid binding column value."
msgstr "Valor de columna vinculante no válido."

#: ../lib/wiking/db.py:309
msgid "Integrity check failed."
msgstr "Fallo en el control de integridad."

#: ../lib/wiking/db.py:685
msgid "This function is temporarily unavailable."
msgstr "Esta función no está temporalmente disponible."

#. Translators: Back button label. Standard computer terminology.
#: ../lib/wiking/db.py:956
msgid "Back"
msgstr "Atrás"

#. Translators: RSS channel is a computer idiom, see Wikipedia.
#: ../lib/wiking/db.py:1049
msgid "An RSS channel is available for this section:"
msgstr "Un canal RSS está disponible para esta sección:"

#: ../lib/wiking/db.py:1053
msgid "more about RSS"
msgstr "Más sobre RSS"

#. Translators: Record as in ,,database record''.
#: ../lib/wiking/db.py:1130
msgid "No records."
msgstr "Ningún registro."

#. Translators: ``Item'' is intentionally general. Could be webpage, notice
#. or something else.
#: ../lib/wiking/db.py:1139
msgid "The item was published."
msgstr "El registro ha sido publicado."

#: ../lib/wiking/db.py:1140
msgid "The item was unpublished."
msgstr "El registro no ha sido publicado."

#: ../lib/wiking/db.py:1149
msgid "Publish"
msgstr "Publicar"

#: ../lib/wiking/db.py:1152
msgid "Make the item visible to website visitors"
msgstr "Hacer el registro visible para los visitantes del sitio web"

#: ../lib/wiking/db.py:1153
msgid "Unpublish"
msgstr "No publicar"

#: ../lib/wiking/db.py:1156
msgid "Make the item invisible to website visitors"
msgstr "Hacer el registro invisible para los visitantes del sitio web"

