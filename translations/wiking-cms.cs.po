msgid "PEM file"
msgstr "PEM soubor"

#: ../lib/wiking/cms/certificates.py:108
msgid "Upload a PEM file containing the certificate"
msgstr "Upload PEM souboru obsahujícího certifikát"

#: ../lib/wiking/cms/certificates.py:109 ../lib/wiking/cms/certificates.py:115
msgid "Certificate"
msgstr "Certifikát"

#: ../lib/wiking/cms/certificates.py:111
