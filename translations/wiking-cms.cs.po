msgstr "Požadavek na certifikát pošlete prostřednictvím stránky %(uri)s"

#: ../lib/wiking/cms/cms.py:288
msgid "Invalid request."
msgstr "Neplatný požadavek."

#: ../lib/wiking/cms/cms.py:292 ../lib/wiking/cms/cms.py:2200
#: ../lib/wiking/cms/cms.py:2222 ../lib/wiking/cms/cms.py:2236
