msgstr "Neaktivní záložka"

#: ../lib/wiking/cms/cms.py:807
msgid "Color Themes"
msgstr "Barevné motivy"

#: ../lib/wiking/cms/cms.py:808
msgid "Manage available color themes."
msgstr "Správa dostupných barevných motivů."

#: ../lib/wiking/cms/cms.py:812 ../lib/wiking/cms/cms.py:1759
msgid "Active"
msgstr "Aktivní"

