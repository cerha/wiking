msgstr "Platný od"

#: ../lib/wiking/cms/certificates.py:121
msgid "Valid until"
msgstr "Platný do"

#: ../lib/wiking/cms/certificates.py:123
msgid "Trusted"
msgstr "Důvěryhodný"

#: ../lib/wiking/cms/certificates.py:124
