msgstr "El contenido tiene que tener formato de texto estructurado de LCG. Consulte el %(manual)s."

#: ../lib/wiking/cms/cms.py:75
msgid "unknown"
msgstr "desconocido"

#: ../lib/wiking/cms/cms.py:94
msgid "Look &amp; Feel"
msgstr "Mira &amp; Siente"

#: ../lib/wiking/cms/cms.py:95
msgid "Customize the appearance of your site."
msgstr "Configurar la apariencia de su sitio web."

#: ../lib/wiking/cms/cms.py:96
