#: ../lib/wiking/cms/cms.py:1043 ../lib/wiking/cms/cms.py:1074
#, fuzzy
msgid "Basic Options"
msgstr "Beállítások"

#: ../lib/wiking/cms/cms.py:1044 ../lib/wiking/cms/cms.py:1069
#: ../lib/wiking/cms/cms.py:1075
msgid "Menu position"
